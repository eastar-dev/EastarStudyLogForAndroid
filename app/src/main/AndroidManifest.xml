<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dev.eastar.studypush">

    <application
        android:name=".base.AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/BTheme">
        <activity
            android:name=".main.AppMain"
            android:label="@string/title_activity_app_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.profile.Profile"
            android:label="@string/title_activity_profile"
            android:theme="@style/BTheme.NoActionBar" />
    </application>

</manifest>